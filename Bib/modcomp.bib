	month = jan,
	year = {2012},
	keywords = {Allometry, compound leaf, fluctuating asymmetry, Geometric morphometrics, leaf shape, phylogenetic uncertainty, plasticity, Principal component analysis, Procrustes fit, Rosaceae},
