	pages = {26--34},
	file = {Full Text PDF:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/I56E8UB2/Bastir and Rosas - 2005 - Hierarchical nature of morphological integration a.pdf:application/pdf;Snapshot:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/2IR2VA27/abstract.html:text/html}
