@article{walker_ability_2000,
	title = {Ability of {Geometric} {Morphometric} {Methods} to {Estimate} a {Known} {Covariance} {Matrix}},
	volume = {49},
	issn = {1063-5157, 1076-836X},
	url = {http://sysbio.oxfordjournals.org/content/49/4/686},
	doi = {10.1080/106351500750049770},
	abstract = {Landmark-based morphometric methods must estimate the amounts of translation, rotation, and scaling (or, nuisance) parameters to remove nonshape variation from a set of digitized figures. Errors in estimaties of these nuisance variables will be reflected in the covariance structure of the coordinates, such as the residuals from a superimposition, or any linear combination of the coordinates, such as the partial warp and standard uniform scores. A simulation experiment was used to compare the ability of the generalized resistant fit (GRF) and a relative warp analysis (RWA) to estimate known covariance matrices with various correlations and variance structures. Random covariance matrices were perturbed so as to vary the magnitude of the average correlation among coordinates, the number of landmarks with excessive variance, and the magnitude of the excessive variance. The covariance structure was applied to random figures with between 6 and 20 landmarks. The results show the expected performance of GRF and RWA across a broad spectrum of conditions. The performance of both GRF and RWA depended most strongly on the number of landmarks. RWA performance decreased slightly when one or a few landmarks had excessive variance. GRF performance peaked when ∼25\% of the landmarks had excessive variance. In general, both RWA and GRF performed better at estimating the direction of the first principal axis of the covariance matrix than the structure of the entire covariance matrix. RWA tended to outperform GRF when {\textgreater} ∼ 75\% of the coordinates had excessive variance. When {\textless} ∼75\% of the coordinates had excessive variance, the relative performance of RWA and GRF depended on the magnitude of the excessive variance; when the landmarks with excessive variance had standard deviations (σ) ≥ 4σ minimum, GRF regularly outperformed RWA.},
	language = {en},
	number = {4},
	urldate = {2013-10-18},
	journal = {Systematic Biology},
	author = {Walker, Jeffrey A.},
	month = dec,
	year = {2000},
	pmid = {12116434},
	keywords = {Covariance structure, partial warps, principal components analysis, relative warps analysis, resistant fit, shape, simulation, superimposition},
	pages = {686--696},
	file = {Full Text PDF:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/WHBWQWQG/Walker - 2000 - Ability of Geometric Morphometric Methods to Estim.pdf:application/pdf;Snapshot:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/EKIABWG5/686.html:text/html}
}

@article{swiderski_separating_2003,
	title = {Separating {Size} from {Allometry}: {Analysis} of {Lower} {Jaw} {Morphology} in the {Fox} {Squirrel}, {Sciurus} niger},
	volume = {84},
	copyright = {Copyright © 2003 American Society of Mammalogists},
	issn = {0022-2372},
	shorttitle = {Separating {Size} from {Allometry}},
	url = {http://www.jstor.org/stable/1383849},
	abstract = {The fundamentals of mechanics provide a strong basis for the hypothesis that allometry could be an important constraint on evolutionary changes in shape. Empirical evidence that allometry accounts for sufficient shape variation to be an effective constraint is much weaker. Numerous studies claim to show that most shape variation is correlated with size, but nearly all these studies use morphometric methods that confound size and shape. Consequently, the proportion of the shape variation correlated with size remains unknown. To address this issue, I examined ontogenetic change and adult variation in lower jaws of the fox squirrel, Sciurus niger. Jaw morphologies were quantified using traditional distance measurements and geometric shape variables computed from landmark coordinates. The same analyses were performed on each data set. As expected, analyses of the distance measurements found that allometry accounted for nearly all ontogenetic variation. The same analysis on the coordinate data found that allometry accounts for {\textless}50\% of the ontogenetic variation in shape. In analyses of adults, allometry explained {\textgreater}50\% of the variation in distance measurements but {\textless}25\% of the variation in shape. These results confirm that analyses of distance measurements confound size and shape and that this can lead to erroneous claims about the importance of allometry.},
	number = {3},
	urldate = {2013-11-07},
	journal = {Journal of Mammalogy},
	author = {Swiderski, Donald L.},
	month = aug,
	year = {2003},
	note = {ArticleType: research-article / Full publication date: Aug., 2003 / Copyright © 2003 American Society of Mammalogists},
	pages = {861--876},
	file = {JSTOR Full Text PDF:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/JVRE4AI9/Swiderski - 2003 - Separating Size from Allometry Analysis of Lower .pdf:application/pdf}
}

@article{voje_allometric_2013,
	title = {Allometric {Constraints} and the {Evolution} of {Allometry}},
	copyright = {This article is protected by copyright. All rights reserved},
	issn = {1558-5646},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/evo.12312/abstract},
	doi = {10.1111/evo.12312},
	abstract = {Morphological traits often covary within and among species according to simple power laws referred to as allometry. Such allometric relationships may result from common growth regulation, and this has given rise to the hypothesis that allometric exponents may have low evolvability and constrain trait evolution. We formalize hypotheses for how allometry may constrain morphological trait evolution across taxa, and test these using more than three-hundred empirical estimates of static (within-species) allometric relations of animal morphological traits. Although we find evidence for evolutionary changes in allometric parameters on million-year, cross-species time scales, there is limited evidence for microevolutionary changes in allometric slopes. Accordingly, we find that static allometries often predict evolutionary allometries on the subspecies level, but less so across species. Although there is a large body of work on allometry in a broad sense that includes all kinds of morphological trait-size relationships, we found relatively little information about the evolution of allometry in the narrow sense of a power relationship. Despite the many claims of microevolutionary changes of static allometries in the literature, hardly any of these apply to narrow-sense allometry, and we argue that the hypothesis of strongly constrained static allometric slopes remains viable.This article is protected by copyright. All rights reserved.},
	language = {en},
	urldate = {2013-11-13},
	journal = {Evolution},
	author = {Voje, Kjetil L. and Hansen, Thomas F. and Egset, Camilla K. and Bolstad, Geir H. and Pélabon, Christophe},
	year = {2013},
	keywords = {Adaptation, Evolutionary constraint, Macroevolution, Microevolution, Ontogenetic allometry, Static allometry},
	pages = {n/a--n/a},
	file = {Snapshot:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/2GT27SK8/abstract.html:text/html;Voje et al. - 2013 - Allometric Constraints and the Evolution of Allome.pdf:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/C55IXKPX/Voje et al. - 2013 - Allometric Constraints and the Evolution of Allome.pdf:application/pdf}
