@article{cheverud_pleiotropic_2004,
	title = {Pleiotropic effects on mandibular morphology {II}: differential epistasis and genetic variation in morphological integration.},
	volume = {302},
	issn = {1552-5007},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/15384169},
	doi = {10.1002/jez.b.21008},
	abstract = {The evolution of morphological modularity through the sequestration of pleiotropy to sets of functionally and developmentally related traits requires genetic variation in the relationships between traits. Genetic variation in relationships between traits can result from differential epistasis, where epistatic relationships for pairs of loci are different for different traits. This study maps relationship quantitative trait loci (QTLs), specifically QTLs that affect the relationship between individual mandibular traits and mandible length, across the genome in an F2 intercross of the LG/J and SM/J inbred mouse strains (N = 1045). We discovered 23 relationship QTLs scattered throughout the genome. All mandibular traits were involved in one or more relationship QTL. When multiple traits were affected at a relationship QTL, the traits tended to come from a developmentally restricted region of the mandible, either the muscular processes or the alveolus. About one-third of the relationship QTLs correspond to previously located trait QTLs affecting the same traits. These results comprise examples of genetic variation necessary for an evolutionary response to selection on the range of pleiotropic effects.},
	number = {5},
	journal = {Journal of experimental zoology. Part B, Molecular and developmental evolution},
	author = {Cheverud, J M and Ehrich, T H and Vaughn, T T and Koreishi, S F and Linsey, R B and Pletscher, L S},
	month = sep,
	year = {2004},
	keywords = {Animals, Biological Evolution, Body Weights and Measures, Chromosome Mapping, Crosses, Epistasis, Genetic, Genetic Variation, Inbred Strains, Mandible, Mandible: anatomy \& histology, Mice, Microsatellite Repeats, Microsatellite Repeats: genetics, Models, Morphogenesis, Quantitative Trait Loci, Quantitative Trait Loci: genetics},
	pages = {424--435},
	file = {Attachment:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/KIZEFMN6/Cheverud et al.. 2004.pdf:application/pdf}
}

@article{cheverud_comparing_2007,
	title = {Comparing covariance matrices: {Random} skewers method compared to the common principal components model},
	volume = {30},
	doi = {10.1590/S1415-47572007000300027},
	abstract = {Comparisons of covariance patterns are becoming more common as interest in the evolution of relationships between traits and in the evolutionary phenotypic diversification of clades have grown. We present parallel analyses of covariance matrix similarity for cranial traits in 14 New World Monkey genera using the Random Skewers (RS), T-statistics, and Common Principal Components (CPC) approaches. We find that the CPC approach is very powerful in that with adequate sample sizes, it can be used to detect significant differences in matrix structure, even between matrices that are virtually identical in their evolutionary properties, as indicated by the RS results. We suggest that in many instances the assumption that population covariance matrices are identical be rejected out of hand. The more interesting and relevant question is, How similar are two covariance matrices with respect to their predicted evolutionary responses? This issue is addressed by the random skewers method described here.},
	journal = {Genetics and Molecular Biology},
	author = {Cheverud, J M and Marroig, G},
	year = {2007},
	keywords = {common principal components, covariance matrix, new world monkeys, quantitative genetics, random skewers},
	pages = {461--469},
	file = {Attachment:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/9P55QVEZ/Cheverud, Marroig. 2007.pdf:application/pdf}
}

