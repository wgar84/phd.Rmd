	keywords = {Animal Ecology, Animal Genetics and Genomics, Anthropology, Cranial base, Cranium, Evolutionary Biology, Facial block, Facial projection, Hominin, Human Genetics, Zoology},
	pages = {1--12},
	file = {Neaux et al. - 2015 - Implications of the Relationship Between Basicrani.pdf:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/KATEIAVS/Neaux et al. - 2015 - Implications of the Relationship Between Basicrani.pdf:application/pdf;Snapshot:/home/guilherme/Dropbox/zotero/9y8qrhli.default/zotero/storage/ZU57PKB9/s10764-015-9886-5.html:text/html}
